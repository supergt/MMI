if (!$('#dialog').dialog('isOpen')) {
	$('#dialog').html("<%= escape_javascript(render :partial => 'show') %>")
		.dialog('option', 'title', 'All Earnings')
		.dialog('open');

	$('.ui-state-default').hover(
	function(){ $(this).addClass('ui-state-hover'); },
	function(){ $(this).removeClass('ui-state-hover'); });

	$(".itemsTableHead th a").live("click", function() {
	    $.getScript(this.href);
	    return false;
	});
} else {
	$('.itemsEarnings').html("<%= escape_javascript(render :partial => 'table') %>");
}